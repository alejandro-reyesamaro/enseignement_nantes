// PARFAIT
var n = SaisieEntier("Nombre: ");
var divs = 1;
for (var i = 2; i < n; i++)
  if (n % i == 0)
    divs = divs + i;

if (divs == n)
  Ecrire("Le nombre est parfait");
else
  Ecrire("Le nombre n'est pas parfait");



// PREMIER
var n = SaisieEntier("Nombre: ");
var premier = true;

for (var i = 2; i < Math.sqrt(n); i++)
  if (n % i == 0){
  	premier = false;
    break;
  }

if (premier)
  Ecrire("Le nombre est premier");
else
  Ecrire("Le nombre n'est pas premier");



